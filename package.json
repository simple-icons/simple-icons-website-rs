  },
  "devDependencies": {
    "@playwright/test": "1.32.3",
    "@types/node": "18.15.5",
    "anywhere": "1.6.0",
    "autoprefixer": "10.4.14",
    "esbuild": "0.17.16",
    "husky": "8.0.3",
    "is-ci": "3.0.1",
    "path-exists-cli": "2.0.0",
    "postcss": "8.4.21",
    "postcss-cli": "10.1.0",
    "postcss-import": "15.1.0",
    "prettier": "2.8.7",
    "tailwindcss": "3.3.1"
  },
  "dependencies": {
    "blob-stream": "0.1.3",
    "fast-fuzzy": "1.12.0",
    "pdfkit": "0.13.0",
    "simple-icons": "8.9.0"
  }
